// ======================== Indian Apache A ========================
Skin("Indian_Apache_A")
{
	!include("man_surfaces.skt")
	!include("man_disable_anims_on_lod.skt")
	!include("man_shoes_f.skt")
	!include("man_pants_g.skt")
	!include("man_torso_h.skt")
	!include("man_hands_a.skt")
	!include("man_head_i.skt")
	!include("man_hair_b.skt")
	
	Replace("man_hands_a.mat", "man_hands_a2.mat")	
	
	Hide("HolsterL")
	Hide("HolsterR")
	
    ColorPreset("Color_1")
    {
       ColorI(0, 162, 128, 100)
       ColorI(2, 18, 47, 17)
       ColorI(3, 57, 61, 31)
    }
    ColorPreset("Color_2")
    {
       ColorI(0, 255, 255, 255)
       ColorI(2, 64, 0, 0)
       ColorI(3, 90, 107, 52)
    }
    ColorPreset("Color_3")
    {
       ColorI(0, 255, 255, 255)
       ColorI(2, 74, 75, 24)
       ColorI(3, 255, 255, 255)
    }
}
Skin("Indian_Apache_A_player")
{
	FilterInEditor()
	!include("man_surfaces.skt")
