	!include("man_shoes_f.skt")
	!include("man_pants_g.skt")
	!include("man_torso_h.skt")
	!include("man_hands_a.skt")
	!include("man_head_i.skt")
	!include("man_hair_b.skt")
	!include("man_player.skt")	
	
	Replace("man_hands_a.mat", "man_hands_a2.mat")	
	
	Hide("HolsterL")
	Hide("HolsterR")
}
